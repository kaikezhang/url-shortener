{
  "name": "url-shorter",
  "version": "1.0.0",
  "description": "Production-ready URL Shortener Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:staging": "NODE_ENV=staging nodemon --env-file=.env.staging src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:staging": "NODE_ENV=staging node --env-file=.env.staging dist/index.js",
    "migrate": "ts-node src/database/migrate.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:start:staging": "pm2 start ecosystem.config.js --env staging",
    "pm2:stop": "pm2 stop url-shortener",
    "pm2:restart": "pm2 restart url-shortener",
    "pm2:reload": "pm2 reload url-shortener",
    "pm2:delete": "pm2 delete url-shortener",
    "pm2:logs": "pm2 logs url-shortener",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "nanoid": "^3.3.11",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
